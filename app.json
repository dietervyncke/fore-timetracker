{
  "expo": {
    "name": "For√© timetracker",
    "description": "Company time tracker",
    "slug": "fore-timetracker",
    "version": "0.0.14",
    "privacy": "public",
    "primaryColor": "#ffffff",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.dietervyncke.foretimetracker",
      "requireFullScreen": true
    },
    "android": {
      "versionCode": 8,
      "package": "com.dietervyncke.foretimetracker",
      "permissions": [
        "CAMERA",
        "CAMERA_ROLL",
        "MEDIA_LIBRARY"
      ]
    },
    "githubUrl": "https://github.com/dietervyncke/fore-timetracker"
  }
}
